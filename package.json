{
  "name": "temporal-learning",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "gen": "docusaurus build",
    "build": "npm-run-all --serial getsnips gen",
    "swizzle": "docusaurus swizzle",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "getsnips": "snipsync",
    "clearsnips": "snipsync --clear",
    "check_links": "lychee docs --exclude-loopback --exclude-mail",
    "check_links_in_build": "lychee build --exclude-loopback --exclude-mail --exclude 'temporal-learning|algolia\\.net'",
    "scan": "npm-run-all -c --serial build check_links_in_build clearsnips",
    "get_courses": "node get_course_pages_from_lms.js",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.3",
    "@docusaurus/preset-classic": "^2.4.3",
    "@mdx-js/react": "^1.6.22",
    "chart.js": "3.7.1",
    "clsx": "^1.1.1",
    "prism-react-renderer": "^1.3.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-player": "^2.10.1",
    "readdirp": "^3.6.0",
    "remark-typescript-tools": "1.0.9",
    "snipsync": "^1.8.0",
    "typescript": "4.x",
    "webpack-font-preload-plugin": "^1.5.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.4.3",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
