{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register/transpile-only"],
      "cwd": "${workspaceRoot}",
      "skipFiles": ["<node_internals>/**"],
      "args": ["${file}"],
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
